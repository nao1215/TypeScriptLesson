{
  "name": "lesson-70-development-workflow",
  "version": "1.0.0",
  "description": "TypeScript Lesson 70: 開発ワークフロー (Development Workflow)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "workflow:init": "node scripts/init-workflow.js",
    "pipeline:test": "node scripts/test-pipeline.js",
    "analyze:team": "node scripts/analyze-team.js",
    "quality:check": "npm run lint && npm run type-check && npm run test",
    "lint": "eslint src/**/*.ts",
    "type-check": "tsc --noEmit",
    "test": "jest"
  },
  "keywords": ["typescript", "development-workflow", "cicd", "code-review", "quality-gates"],
  "devDependencies": {
    "@types/node": "^20.6.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "typescript": "^5.2.2",
    "ts-node": "^10.9.1",
    "jest": "^29.7.0"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "simple-git": "^3.19.1"
  }
}